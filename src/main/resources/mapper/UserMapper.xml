<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springboot.mybatis.jvhuaxia.springbootmybatis.mapper.UserMapper">
  <select id="getUser" parameterType="java.lang.Integer"
    resultType="com.springboot.mybatis.jvhuaxia.springbootmybatis.pojo.User">
    SELECT *
    FROM user
    WHERE id = #{id};
  </select>

</mapper>